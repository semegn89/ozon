{
  "version": 2,
  "builds": [
    {
      "src": "webapp/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "webapp/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/webapp/api.py"
    },
    {
      "src": "/webapp/(.*)",
      "dest": "/webapp/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/webapp/index.html"
    }
  ],
  "env": {
    "DB_URL": "@db_url",
    "BOT_TOKEN": "@bot_token",
    "ADMIN_CHAT_IDS": "@admin_chat_ids"
  }
}
